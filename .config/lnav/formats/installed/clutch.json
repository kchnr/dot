{
  "$schema": "https://lnav.org/schemas/format-v1.schema.json",
  "clutch": {
    "title": "Clutch log format",
    "description": "Clutch",
    "file-type": "json",
    "hide-extra": true,
    "ordered-by-time": false,
    "level-field": "level",
    "level": {
      "warning": "WARN",
      "error": "ERROR",
      "info": "INFO",
      "debug": "DEBUG"
    },
    "line-format": [
      {
        "field": "timestamp"
      },
      " ",
      {
        "field": "level"
      },
      " ",
      {
        "field": "status"
      },
      " ",
      {
        "field": "file_path"
      },
      " ",
      {
        "field": "writeKey"
      },
      " ",
      {
        "field": "url"
      },
      " ",
      {
        "field": "requestId"
      },
      " ",
      {
        "field": "message"
      },
      " ",
      {
        "field": "tags"
      },
      " ",
      {
        "field": "error"
      },
      " ",
      {
        "field": "extra"
      }
    ],
    "timestamp-field": "timestamp",
    "opid-field": "requestId",
    "value": {
      "timestamp": {
        "kind": "string"
      },
      "level": {
        "kind": "string"
      },
      "requestId": {
        "kind": "string"
      },
      "message": {
        "kind": "string"
      },
      "accountOpeningApplicationId": {
        "kind": "string"
      },
      "customerId": {
        "kind": "string"
      },
      "status": {
        "kind": "string"
      },
      "environment": {
        "kind": "string"
      },
      "file_path": {
        "kind": "string"
      },
      "url": {
        "kind": "string"
      },
      "writeKey": {
        "kind": "string"
      },
      "tags": {
        "kind": "json"
      },
      "error": {
        "kind": "json"
      },
      "extra": {
        "kind": "json"
      },
      "events": {
        "kind": "json"
      }
    },
    "sample": [
      {
        "line": "{\"level\":\"DEBUG\",\"timestamp\":\"2024-08-29T15:00:03.391Z\",\"message\":\"rudderstack keys\",\"writeKey\":\"1XXXXXXXXXXXXXXXXXXXXXXXXXb\",\"url\":\"https:\/\/withclutchungm.dataplane.rudderstack.com\"}"
      },
      {
        "line": "{\"level\":\"DEBUG\",\"timestamp\":\"2024-08-29T15:00:03.412Z\",\"message\":\"rudderstack keys\",\"writeKey\":\"1XXXXXXXXXXXXXXXXXXXXXXXXXb\",\"url\":\"https:\/\/withclutchungm.dataplane.rudderstack.com\"}"
      },
      {
        "line": "{\"level\":\"INFO\",\"timestamp\":\"2024-08-29T15:00:13.543Z\",\"message\":\"LoansDataPoller: Polling is disabled\",\"requestId\":\"3f47a90d-56b7-4666-a788-6c7523c3ef1f\",\"dd\":{\"trace_id\":\"527831867057455466\",\"span_id\":\"527831867057455466\",\"service\":\"clutch-api\"}}"
      }
    ]
  }
}
